<%-include('./partials/header',{title: "Error"})%>
    <div class="alert alert-danger" role="alert">
        <h4 class="alert-heading">
            <%=err.message%>
        </h4>
        <% if (process.env.NODE_ENV !== "production") {%>
            <p>
                <%= err.stack  %>
            </p>
            <% } %>
                <hr>
                <p class="mb-0">Oh No, Something went wrong.</p>
    </div>
    <%-include('./partials/footer')%>